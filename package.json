{
  "name": "backend-test",
  "version": "3.0.4",
  "description": "PeanutHub Test",
  "author": "PeanutHub - Dev Team",
  "license": "MIT",
  "private": true,
  "main": "./runners",
  "engines": {
    "node": "14.17.6",
    "npm": "6.x"
  },
  "scripts": {
    "start": "npm run service:main",
    "doc": "apidoc -i ./src/services/api/routers -o ./apidoc/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "depcheck": "./node_modules/.bin/npm-check",
    "test": "npm run test:dotonly && npm run lint && npm run docker:test:unit",
    "test:unit": "mocha ./test/test.config.js ./test/test.config.integ.js ./test/unit --recursive --timeout 90000",
    "test:dotonly": "dot-only-hunter test",
    "test:unit:coverage": "nyc --temp-dir=coverage/.nyc_outputUnit --report-dir=coverage/unit npm run test:unit",
    "test:coverage:check": "nyc check-coverage --temp-dir=coverage/unit --statements 95 --branches 82 --functions 91 --lines 95",
    "test:circleci": "circleci config validate",
    "service:migrations": "migrate-mongo up",
    "service:main": "node runners/main",
    "service:api": "nodemon runners/api",
    "docker:test:unit": "docker-compose -f docker-compose.test.yml up --exit-code-from unit --build unit",
    "docker:test:dotonly": "docker-compose -f docker-compose.test.yml down && docker-compose -f docker-compose.test.yml up --exit-code-from dotonly --build dotonly && docker-compose -f docker-compose.test.yml down",
    "test:manual": "mocha ./test/test.config.js ./test/manual --recursive --timeout 90000",
    "docker:dependencies": "docker-compose down && docker-compose up -d --build mongoose",
    "docker:services": "docker-compose up --build -d service-api",
    "wait:db": "test/scripts/waitUp.sh mongoose 27017",
    "wait:api": "test/scripts/waitUp.sh service-api 3000"
  },
  "nyc": {
    "cache": true,
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "json",
      "text-summary"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "src/helpers/origin.js",
      "src/helpers/request.js",
      "src/models/index.js",
      "src/modules/db.js",
      "src/modules/runner.js"
    ]
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@sentry/node": "^6.13.3",
    "apidoc": "^0.50.4",
    "async": "^3.2.1",
    "aws-sdk": "^2.1087.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-date-string": "^0.1.0",
    "chai-xml": "^0.4.0",
    "cloudinary": "^1.27.0",
    "decimal.js": "^10.3.1",
    "dot-only-hunter": "^1.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-mocha": "^8.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethers": "^5.5.4",
    "eth-sig-util": "^3.0.1",
    "faker": "^5.5.3",
    "geoip-lite": "^1.4.2",
    "istanbul-combine": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-charset": "^2.0.0",
    "koa-compose": "^4.1.0",
    "koa-ctx-cache-control": "^1.0.1",
    "koa-jwt": "^4.0.1",
    "lodash": "^4.17.21",
    "lodash.pick": "^4.4.0",
    "logzio-nodejs": "^2.0.1",
    "mocha": "^9.2.1",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "mongodb-memory-server": "^7.4.3",
    "mongoose": "^5.12.11",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "node-rsa": "^1.1.1",
    "npm-check": "^4.1.4",
    "nyc": "^15.1.0",
    "onesignal-node": "^3.3.0",
    "promise-retry": "^2.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "sinon": "^7.4.2",
    "sinon-mongoose": "^2.3.0",
    "speakeasy": "^2.0.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "throng": "^5.0.0",
    "toobusy-js": "^0.5.1",
    "ua-parser-js": "^0.7.28",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "migrate-mongo": "^8.2.3",
    "nodemon": "^2.0.15"
  }
}
